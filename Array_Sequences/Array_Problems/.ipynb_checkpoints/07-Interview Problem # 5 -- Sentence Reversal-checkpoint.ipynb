   "source": [
    "def reverse_sent(sentence):\n",
    "    sentence = sentence.split()\n",
    "    sentence.reverse()\n",
    "    \n",
    "    return \" \".join(sentence)\n",
    "\n",
    "reverse_sent('Python is the best!')"
