    "def permute(s):\n",
    "    \n",
    "    out = []\n",
    "    \n",
    "    # Base case\n",
    "    if len(s) == 1:\n",
    "        out = [s]\n",
    "        \n",
    "    else:\n",
    "        \n",
    "        for i, let in enumerate(s):\n",
    "            for perm in permute( s[:1] + s[i+1:]):\n",
    "                out += [let+perm]\n",
    "                \n",
    "        return out "
   ]
